{ 
  "cosmos-installer": {
  },
  "services": {
    "{ServiceName}": {
      "image": "influxdb:latest",
      "container_name": "{ServiceName}",
      "restart": "unless-stopped",
      "UID": 1000,
      "GID": 1000,
      "security_opt": [
        "no-new-privileges:true"
      ],
      "environment": [
        "PUID=1000",
        "PGID=1000"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://aseracorp.github.io/resiSTORE/servapps/{ServiceName}/icon.png"
      },
      "volumes": [
        {
          "source": "{ServiceName}-config",
          "target": "/etc/influxdb2",
          "type": "volume"
        },
        {
          "source": "{ServiceName}-data",
          "target": "/var/lib/influxdb2",
          "type": "volume"
        }
      ],
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "{ServiceName} im Netzwerk ver√∂ffentlichen",
          "useHost": true,
          "target": "http://{ServiceName}:8086",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          },
          "AuthEnabled": true
        }
      ]
    }
  }
}